{
  "source": {
    "include": ["passport"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|jsdocs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./public/jsdocs/"
  }
}
